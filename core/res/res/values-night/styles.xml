<?xml version="1.0" encoding="UTF-8"?><resources>
  <style name="Theme.DeviceDefault.Settings.NoActionBar" parent="@android:style/Theme.DeviceDefault.Settings">
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
  </style>
  <style name="Theme.DeviceDefault.Settings" parent="@*android:style/Theme.Material.Settings"/>
  <style name="Theme.SettingsBase" parent="@*android:style/Theme.Material.Settings"/>
  <style name="Theme.Material.Settings.NoActionBar" parent="@android:style/Theme.Material.Settings">
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
  </style>
  <style name="Theme.Material.Settings" parent="@*android:style/Theme.DeviceDefault.Light">
    <item name="android:colorBackground">#FAFAFA</item>
    <item name="android:colorPrimary">@*android:color/material_blue_grey_900</item>
    <item name="android:colorPrimaryDark">@*android:color/material_blue_grey_950</item>
    <item name="android:colorAccent">@*android:color/material_deep_teal_500</item>
    <item name="android:windowLightStatusBar">true</item>
  </style>
   <style name="Theme.DeviceDefault.Light" parent="@android:style/Theme.Material.Light">
    <item name="*android:listChoiceIndicatorSingle">@*android:drawable/btn_radio_material_anim</item>
    <item name="*android:listChoiceIndicatorMultiple">@*android:drawable/btn_check_material_anim</item>
    <item name="android:colorBackgroundFloating">#FAFAFA</item>
    <item name="android:textColorPrimary">@*android:color/primary_text_light</item>
    <item name="android:textColorSecondary">@*android:color/primary_text_light</item>
    <item name="android:textColorTertiary">@*android:color/primary_text_light</item>
    <item name="android:colorForeground">#546e7a</item>
    <item name="android:textColorPrimaryDisableOnly">#4d546e7a</item>
    <item name="android:colorBackground">#FAFAFA</item>
    <item name="android:actionBarSize">@*android:dimen/action_bar_default_height_material</item>
    <item name="android:colorBackgroundCacheHint">@null</item>
    <item name="android:windowShowWallpaper">false</item>
    <item name="android:colorPrimary">#ffffff</item>
    <item name="android:windowAnimationStyle">@*android:style/Animation.Material.Activity</item>
    <item name="android:textColorHintInverse">#BD000000</item>
    <item name="android:colorPrimaryDark">#f0f0f0</item>
    <item name="android:windowBackground">@*android:drawable/background_holo_dark</item>
    <item name="android:buttonStyle">@*android:style/Widget.DeviceDefault.Button</item>
    <item name="android:textAppearanceButton">@*android:style/TextAppearance.DeviceDefault.Widget.Button</item>
    <item name="android:actionOverflowButtonStyle">@*android:style/Widget.DeviceDefault.ActionButton.Overflow</item>
    <item name="android:checkboxStyle">@*android:style/Widget.DeviceDefault.CompoundButton.CheckBox</item>
    <item name="android:alertDialogTheme">@*android:style/Theme.DeviceDefault.Dialog.Alert</item>
    <item name="android:dialogTheme">@*android:style/Theme.DeviceDefault.Dialog</item>
    <item name="android:actionBarStyle">@*android:style/Widget.DeviceDefault.ActionBar</item>
    <item name="android:toolbarStyle">@*android:style/Widget.DeviceDefault.ActionBar.Solid</item>
    <item name="android:actionBarTheme">@*android:style/Widget.DeviceDefault.Light.ActionBar</item>
    <item name="android:popupTheme">@*android:style/ThemeOverlay.Material.Light</item>
  </style>
  <style name="Theme.DeviceDefault" parent="@android:style/Theme.Material">
    <item name="*android:listChoiceIndicatorSingle">@*android:drawable/btn_radio_material_anim</item>
    <item name="*android:listChoiceIndicatorMultiple">@*android:drawable/btn_check_material_anim</item>
    <item name="android:colorBackgroundFloating">#303030</item>
    <item name="android:colorForeground">#ffffff</item>
    <item name="android:textColorPrimary">@*android:color/primary_text_dark</item>
    <item name="android:textColorSecondary">@*android:color/primary_text_dark</item>
    <item name="android:textColorTertiary">@*android:color/primary_text_dark</item>
    <item name="android:textColorPrimaryDisableOnly">#4dffffff</item>
    <item name="android:colorBackground">#303030</item>
    <item name="android:colorBackgroundCacheHint">@null</item>
    <item name="android:windowShowWallpaper">false</item>
    <item name="android:colorPrimary">#ffffff</item>
    <item name="android:windowAnimationStyle">@*android:style/Animation.Material.Activity</item>
    <item name="android:textColorHintInverse">#BD000000</item>
    <item name="android:colorPrimaryDark">#f0f0f0</item>
    <item name="android:windowBackground">@*android:drawable/background_holo_dark</item>
    <item name="android:buttonStyle">@*android:style/Widget.DeviceDefault.Button</item>
    <item name="android:textAppearanceButton">@*android:style/TextAppearance.DeviceDefault.Widget.Button</item>
    <item name="android:actionBarSize">@*android:dimen/action_bar_default_height_material</item>
    <item name="android:checkboxStyle">@*android:style/Widget.DeviceDefault.CompoundButton.CheckBox</item>
    <item name="android:actionBarStyle">@*android:style/Widget.DeviceDefault.ActionBar</item>
    <item name="android:alertDialogTheme">@*android:style/Theme.DeviceDefault.Dialog.Alert</item>
    <item name="android:dialogTheme">@*android:style/Theme.DeviceDefault.Dialog</item>
    <item name="android:toolbarStyle">@*android:style/Widget.DeviceDefault.ActionBar.Solid</item>
    <item name="android:actionBarTheme">@*android:style/Widget.DeviceDefault.Light.ActionBar</item>
    <item name="android:popupTheme">@*android:style/ThemeOverlay.Material.Light</item>
    <item name="android:actionOverflowButtonStyle">@*android:style/Widget.DeviceDefault.ActionButton.Overflow</item>
  </style>
  <style name="TextAppearance.DeviceDefault.Widget.ActionBar.Title" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Title">
    <item name="android:textColor">?android:attr/colorAccent</item>
    <item name="android:textColorHighlight">?android:attr/colorAccent</item>
    <item name="android:textColorHint">?android:attr/colorAccent</item>
    <item name="android:textColorLink">?android:attr/colorAccent</item>
  </style>
  <style name="TextAppearance.DeviceDefault.Widget.ActionBar.Subtitle" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Subtitle">
    <item name="android:textColor">#669c27b0</item>
    <item name="android:textColorHighlight">#669c27b0</item>
    <item name="android:textColorHint">#669c27b0</item>
    <item name="android:textColorLink">#669c27b0</item>
  </style>
  <style name="Widget.DeviceDefault.Light.ActionBar" parent="@android:style/ThemeOverlay.Material.Dark.ActionBar">
    <item name="android:colorControlNormal">?android:attr/colorAccent</item>
    <item name="android:textColorPrimary">?android:attr/colorAccent</item>
    <item name="android:colorBackground">#ffffff</item>
  </style>
  <style name="Widget.DeviceDefault.ActionBar" parent="@android:style/Widget.Material.ActionBar">
    <item name="android:colorControlNormal">?android:attr/colorAccent</item>
    <item name="android:textColorPrimary">?android:attr/colorAccent</item>
    <item name="android:backgroundStacked">#ffffff</item>
    <item name="android:backgroundSplit">#ffffff</item>
    <item name="android:background">#ffffff</item>
    <item name="android:colorBackground">#ffffff</item>
    <item name="android:textColorSecondary">?android:attr/colorAccent</item>
    <item name="android:elevation">@*android:dimen/action_bar_elevation_material</item>
    <item name="android:textColor">?android:attr/colorAccent</item>
    <item name="android:titleTextStyle">@*android:style/TextAppearance.DeviceDefault.Widget.ActionBar.Title</item>
    <item name="android:subtitleTextStyle">@*android:style/TextAppearance.DeviceDefault.Widget.ActionBar.Subtitle</item>
  </style>
  <style name="Widget.DeviceDefault.ActionBar.Solid" parent="@android:style/Widget.Material.Toolbar">
    <item name="android:backgroundStacked">#ffffff</item>
    <item name="android:backgroundSplit">#ffffff</item>
    <item name="android:background">#ffffff</item>
    <item name="android:colorBackground">#ffffff</item>
    <item name="android:textColorSecondary">?android:attr/colorAccent</item>
    <item name="android:elevation">@*android:dimen/action_bar_elevation_material</item>
    <!--item name="*android:titleMargin">4dp</item-->
    <item name="android:textColorPrimary">?android:attr/colorAccent</item>
    <item name="android:colorControlNormal">?android:attr/colorAccent</item>
    <item name="android:textColor">?android:attr/colorAccent</item>
    <!--item name="android:titleTextStyle">@*android:style/TextAppearance.DeviceDefault.Widget.ActionBar.Title</item>
    <item name="android:subtitleTextStyle">@*android:style/TextAppearance.DeviceDefault.Widget.ActionBar.Subtitle</item-->
  </style>
</resources>
